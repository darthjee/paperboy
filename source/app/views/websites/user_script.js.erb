// ==UserScript==
// @name         <%= user_script_wrapper.name %>
// @include      <%= user_script_wrapper.location %>
// ==/UserScript==

(function(PaperboyUserScript, undefined) {
  if (PaperboyUserScript === undefined) {
    window.PaperboyUserScript = {};
  }
})(window.PaperboyUserScript);

(function() {
  'use strict';

  <%= render partial: 'paperboy/path_parser' %>
  <%= render partial: 'paperboy/script_loader' %>

  <% user_script_wrapper.scripts.each do |website_script| %>
    ScriptLoader.load(<%= website_script.path.to_json %>, function() {
      <%= website_script.content %>
    });
  <% end %>
})();
